<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" parallel="classes" thread-count="1">
    <listeners>
        <listener class-name="tests.AnnotationTransformer"/>
        <listener class-name="tests.TestListener"/>
    </listeners>
    <test name="Cart">
        <classes>
            <class name="tests.CartTest"/>
            <class name="tests.CheckoutYourInformationTest"/>
        </classes>
    </test>
    <test name="Sort">
        <classes>
            <class name="tests.SortTest"/>
        </classes>
    </test>
    <test name="Product">
        <classes>
            <class name="tests.ProductTest"/>
        </classes>
    </test>
    <test name="Login">
        <classes>
            <class name="tests.LoginTest"/>
        </classes>
    </test>
    <test name="HeaderContainer">
        <classes>
            <class name="tests.HeaderContainerTest"/>
        </classes>
    </test>
    <test name="Checkout">
        <classes>
            <class name="tests.CheckoutTest"/>
        </classes>
    </test>
</suite>
